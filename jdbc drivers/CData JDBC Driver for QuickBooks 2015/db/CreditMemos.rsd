<rsb:script xmlns:rsb="http://www.rssbus.com/ns/rsbscript/2">  

  <rsb:info title="CreditMemos" description="Create, update, delete, and query QuickBooks Credit Memos.">
  
    <!-- Column definitions -->
    <attr name="ID"                    xs:type="string"       readonly="true"      key="true"          references=""                       columnsize="255"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="TxnID"                                    desc="The unique identifier." /> 
    <attr name="ReferenceNumber"       xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="21"       normalized="true"      other:qbxmlversion=""        other:selectcolumnname="RefNumber"                                desc="The transaction reference number."/>
    <attr name="TxnNumber"             xs:type="integer"      readonly="true"      required="false"    references=""                       columnsize=""         normalized="true"      other:qbxmlversion=""        other:selectcolumnname="TxnNumber"                                desc="The transaction number. An identifying number for the transaction, different from the QuickBooks-generated Id." /> 
    <attr name="Date"                  xs:type="date"         readonly="false"     required="false"    references=""                       columnsize=""         normalized="true"      other:qbxmlversion=""        other:selectcolumnname="TxnDate"                                  desc="The date of the transaction. If it is set in the WHERE clause of a SELECT query, the pseudo columns StartDate and EndDate are overwritten with the value." />
    <attr name="CustomerName"          xs:type="string"       readonly="false"     required="true"     references="Customers.FullName"     columnsize="1000"     normalized="true"      other:qbxmlversion=""        other:selectcolumnname="CustomerRef"                              desc="The name of the customer on the credit memo. CustomerName or CustomerId are required to have a value when inserting."/>
    <attr name="CustomerId"            xs:type="string"       readonly="false"     required="true"     references="Customers.ID"           columnsize="255"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="CustomerRef"                              desc="The Id of the customer on the credit memo. CustomerName or CustomerId are required to have a value when inserting."/>
