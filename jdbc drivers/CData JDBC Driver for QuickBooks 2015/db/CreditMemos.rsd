    <attr name="CustomerSalesTaxId"    xs:type="string"       readonly="false"     required="false"    references="SalesTaxCodes.ID"       columnsize="255"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="CustomerSalesTaxCodeRef"                  desc="Reference to sales tax information for the customer."/> 
    <attr name="Class"                 xs:type="string"       readonly="false"     required="false"    references="Class.FullName"         columnsize="1000"     normalized="true"      other:qbxmlversion=""        other:selectcolumnname="ClassRef"                                 desc="A reference to the class of the transaction."/> 
