    <attr name="ItemAggregate"         xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="5000"     normalized="false"     other:qbxmlversion=""        other:selectcolumnname="CreditMemoLineRet,CreditMemoLineGroupRet"     desc="An aggregate of the line item data which can be used for adding a credit memo and its line item data."/>
    <attr name="TransactionCount"      xs:type="integer"      readonly="true"      required="false"    references=""                       columnsize=""         normalized="false"     other:qbxmlversion=""        other:selectcolumnname="LinkedTxn"                                desc="The count of related transactions to the credi tmemo." />
    <attr name="TransactionAggregate"  xs:type="string"       readonly="true"      required="false"    references=""                       columnsize="5000"     normalized="false"     other:qbxmlversion=""        other:selectcolumnname="LinkedTxn"                                desc="An aggregate of the linked transaction data."/>
    <attr name="CustomFields"          xs:type="string"       readonly="false"     required="false"    references=""                       columnsize=""         normalized="true"      other:qbxmlversion=""        other:selectcolumnname="DataExtRet"                               desc="Custom fields returned from QuickBooks and formatted into XML." /> 
