    <attr name="BillingLine5"          xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="41"       normalized="true"      other:qbxmlversion="6.0"     other:selectcolumnname="BillAddress"                              desc="Fifth line of the billing address."/> 
    <attr name="BillingCity"           xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="255"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="BillAddress"                              desc="City name for the billing address of the credit memo."/> 
    <attr name="BillingState"          xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="255"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="BillAddress"                              desc="State name for the billing address of the credit memo."/> 
    <attr name="BillingPostalCode"     xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="30"       normalized="true"      other:qbxmlversion=""        other:selectcolumnname="BillAddress"                              desc="Postal code for the billing address of the credit memo."/> 
    <attr name="BillingCountry"        xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="255"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="BillAddress"                              desc="Country for the billing address of the credit memo."/> 
    <attr name="BillingNote"           xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="41"       normalized="true"      other:qbxmlversion="6.0"     other:selectcolumnname="BillAddress"                              desc="Note for the billing address of the credit memo." />
    <attr name="ShippingAddress"       xs:type="string"       readonly="true"      required="false"    references=""                       columnsize=""         normalized="true"      other:qbxmlversion=""        other:selectcolumnname="ShipAddress"                              desc="Full shipping address returned by QuickBooks."/> 
    <attr name="ShippingLine1"         xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="500"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="ShipAddress"                              desc="First line of the shipping address."/> 
    <attr name="ShippingLine2"         xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="500"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="ShipAddress"                              desc="Second line of the shipping address."/> 
    <attr name="ShippingLine3"         xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="500"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="ShipAddress"                              desc="Third line of the shipping address."/> 
    <attr name="ShippingLine4"         xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="500"      normalized="true"      other:qbxmlversion="2.0"     other:selectcolumnname="ShipAddress"                              desc="Fourth line of the shipping address."/> 
    <attr name="ShippingLine5"         xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="41"       normalized="true"      other:qbxmlversion="6.0"     other:selectcolumnname="ShipAddress"                              desc="Fifth line of the shipping address."/> 
    <attr name="ShippingCity"          xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="255"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="ShipAddress"                              desc="City name for the shipping address of the credit memo."/> 
    <attr name="ShippingState"         xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="255"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="ShipAddress"                              desc="State name for the shipping address of the credit memo."/> 
    <attr name="ShippingPostalCode"    xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="30"       normalized="true"      other:qbxmlversion=""        other:selectcolumnname="ShipAddress"                              desc="Postal code for the shipping address of the credit memo."/> 
    <attr name="ShippingCountry"       xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="255"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="ShipAddress"                              desc="Country for the shipping address of the credit memo."/> 
    <attr name="ShippingNote"          xs:type="string"       readonly="false"     required="false"    references=""                       columnsize="41"       normalized="true"      other:qbxmlversion="6.0"     other:selectcolumnname="ShipAddress"                              desc="Note for the shipping address of the credit memo." />
