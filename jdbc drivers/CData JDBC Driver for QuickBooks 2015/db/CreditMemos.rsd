    <attr name="TimeCreated"           xs:type="datetime"     readonly="true"      required="false"    references=""                       columnsize=""         normalized="true"      other:qbxmlversion=""        other:selectcolumnname="TimeCreated"                              desc="When the credit memo was created."/> 
    
    <!-- Psuedo-Column definitions -->
