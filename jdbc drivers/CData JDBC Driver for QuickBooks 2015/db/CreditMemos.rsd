    <attr name="Amount"                xs:type="double"       readonly="false"     required="false"    references=""                       columnsize=""         normalized="true"      other:qbxmlversion=""        other:selectcolumnname="TotalAmount"                              desc="Total amount for this transaction." /> 
    <attr name="Message"               xs:type="string"       readonly="false"     required="false"    references="CustomerMessages.Name"  columnsize="101"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="CustomerMsgRef"                           desc="A message to the customer."/> 
    <attr name="MessageId"             xs:type="string"       readonly="false"     required="false"    references="CustomerMessages.ID"    columnsize="255"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="CustomerMsgRef"                           desc="Id of the message to the customer."/> 
