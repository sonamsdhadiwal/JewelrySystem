    <attr name="AccountsReceivableId"  xs:type="string"       readonly="false"     required="false"    references="Accounts.ID"            columnsize="255"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="ARAccountRef"                             desc="A reference to the Id of the accounts-receivable account where the money received from this transaction will be deposited." />
    <attr name="ShipMethod"            xs:type="string"       readonly="false"     required="false"    references="ShippingMethods.Name"   columnsize="1000"     normalized="true"      other:qbxmlversion=""        other:selectcolumnname="ShipMethodRef"                            desc="The shipping method."/> 
    <attr name="ShipMethodId"          xs:type="string"       readonly="false"     required="false"    references="ShippingMethods.ID"     columnsize="255"      normalized="true"      other:qbxmlversion=""        other:selectcolumnname="ShipMethodRef"                            desc="The shipping method Id."/> 
